<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
	<inherits name='com.google.gwt.user.User' />
	<define-configuration-property name="loon.reflect.include"
		is-multi-valued="true" />
	<define-configuration-property name="loon.reflect.exclude"
		is-multi-valued="true" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.Screen" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.LObject" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.LRelease" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.LSetting" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.LTexture" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.canvas.Canvas" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.canvas.Image" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.utils.ObjectMap" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.utils.OrderedMap" />
	<extend-configuration-property name="loon.reflect.include"
		value="loon.utils.Array" /> 
	<extend-configuration-property name="loon.reflect.include"
		value="loon.utils.TArray" /> 
	<extend-configuration-property name="loon.reflect.include"
		value="loon.utils.TArrayMap" /> 
    <extend-configuration-property name="loon.reflect.include"
        value="java.util.Collection" />
	<extend-configuration-property name="loon.reflect.include"
        value="java.util.List" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.util.ArrayList" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.util.Map" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.util.HashMap" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.String" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Boolean" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Byte" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Short" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Character" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Integer" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Float" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Double" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.CharSequence" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Enum" />
	<extend-configuration-property name="loon.reflect.include"
		value="java.lang.Object" />
	<generate-with class="loon.gwtref.gen.ReflectionCacheGenerator">
		<when-type-assignable class="loon.gwtref.client.IReflectionCache" />
	</generate-with>
</module>
